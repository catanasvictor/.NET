@model SalonWeb.DomainModel.Appointment
@{
    ViewData["Title"] = "Export";
}

<h1>Export Appointments</h1>
@using (Html.BeginForm())
{
<div class="form-group">


    @Html.DropDownList("exp", new List<SelectListItem>
    {
       new SelectListItem{ Text="Json", Value = "1" },
       new SelectListItem{ Text="Csv", Value = "2" },
       new SelectListItem{ Text="Xml", Value = "3"},     
      
    })
</div>

    <button type="Submit" class="btn btn-success submit">Send</button>
}


